    "test": "jest --verbose assets/js",
    "test:watch": "npm run test --watch",
    "build": "webpack --config webpack.config.js",
    "watch": "webpack --config webpack.config.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jasminenoack/test-site.git"
  },
  "author": "Jasmine Noack",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jasminenoack/test-site/issues"
  },
  "homepage": "https://github.com/jasminenoack/test-site#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-jest": "^14.1.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-object-assign": "6.3.13",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-stage-2": "6.3.13",
    "chai": "^3.5.0",
    "chai-immutable": "^1.6.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.4.0",
    "eslint-plugin-react": "^6.2.0",
    "immutable": "3.7.5",
    "isomorphic-fetch": "^2.2.1",
    "istanbul": "^0.4.5",
    "jest-cli": "^14.1.0",
    "jquery": "^3.1.0",
    "jsdom": "^9.4.2",
    "mock-fetch": "^1.0.0",
    "react": "^15.3.1",
    "react-addons-test-utils": "^15.3.1",
    "react-cookie": "^0.4.8",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.7.0",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "webpack": "^1.13.2",
    "webpack-bundle-tracker": "0.0.93"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-2"
    ],
    "plugins": [
      "transform-object-assign"
    ]
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "collectCoverage": true,
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/chai",
      "<rootDir>/node_modules/chai-immutable",
      "<rootDir>/node_modules/immutable",
      "<rootDir>/node_modules/enzyme"
    ]
  }
}
